{
	"name": "react-todos-redux",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.7.1",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-redux": "^6.0.0",
		"react-router-dom": "^4.3.1",
		"react-scripts": "^2.1.2",
		"react-test-renderer": "^16.7.0",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"start:dev": "concurrently \"json-server -p 8080 -d 500 ./db.json\" \"yarn start\"",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"dev-server": "json-server -p 8080 ./db.json"
	},
	"devDependencies": {
		"concurrently": "^4.1.0",
		"redux-devtools-extension": "^2.13.7"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"eslintConfig": {
		"extends": "react-app"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/index.js",
			"!src/setupTests.js",
			"!src/store.js",
			"!<rootDir>/node_modules/",
			"!<rootDir>/path/to/dir/"
		]
	}
}
